{
    "name": "ultraproject/uconfig",
    "description": "Libreria per la gestione avanzata delle configurazioni in Laravel.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "UltraProject\\UConfig\\": "src/"
        }
    },
    "suggest": {
        "spatie/laravel-permission": "Permette la gestione avanzata di ruoli e permessi"
    },
    "extra": {
        "laravel": {
            "providers": [
                "UltraProject\\UConfig\\Providers\\UConfigServiceProvider"
            ],
            "aliases": {
                "UConfig": "UltraProject\\UConfig\\Facades\\UConfig"
            }
        }
    },
    "authors": [
        {
            "name": "Fabio Cherici",
            "email": "fabiocherici@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "spatie/laravel-permission": "^6.10",
        "ultraproject/uconfig": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "config": {
        "sort-packages": true
    }
    
}
